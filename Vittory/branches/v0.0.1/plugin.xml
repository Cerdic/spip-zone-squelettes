		<inclure>vittory_pipelines.php</inclure>
	</pipeline>
	<categorie>squelette</categorie>
	<necessite id="SPIP" version='[2.0.0;3.0.99]' />
	<necessite id='cfg' version='[1.0.4;]' />
	<utilise id="palette" />
